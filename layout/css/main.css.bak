/* layout */
body, html{width:100%; height:100%; min-width:280px; 
color:#777777; 
 font-family: "Noto Sans Korean",  sans-serif;
font-size:12px;}
body.is-active{overflow:hidden;}
#chn{    font-family: 'Noto Sans Korean'} 
#wrap{width:100%; overflow:hidden;}
#header{width:100%;}
#visual{width:100%;}
#contents{width:100%; }
#footer{width:100%; }
.in_Layer2{width:1720px; margin:0 auto; }
.in_Layer{width:1500px; margin:0 auto;  padding: 0px;}
.in_Layer3{width:1200px; margin:0 auto;}


.cf:after{clear:both; display:block; content:"";}
.flex-box{
display: -webkit-box;
display: -ms-flexbox;
display: flex;
-ms-flex-wrap: wrap;
flex-wrap: wrap;}
.table-cell{display:table-cell; vertical-align:middle;}
a,button,li,img{outline:none;}
button{cursor:pointer;}

#esp{font-family: arial}

#skip{}
#skip a{display:block; position:absolute; top:0; width:0; height:0; margin-top:-1px; text-align:center; white-space:nowrap; overflow:hidden; text-decoration:none;}
#skip a:focus, #skip a:active{position:relative; top:0; left:0;  width:100%; height:40px; line-height:40px; font-size:18px; font-weight:bold; color:#fff; background-color:#040404; z-index:100;}

/****************************************************************************** header ******************************************************************************/



#header{position:fixed; border-bottom:1px solid rgba(255, 255, 255, 0.3);  z-index: 9900;}
#header{width:100%; height:100px; background:transparent; top:0;  transition:background 0.3s ease-in-out ,height 0.3s ease-in-out;}
#header .in_Layer2:after{clear:both; display:block; content:"";}
#header .in_Layer2{position:relative; height:100%;}
/*:: h_top ::*/
#header .h_top{ padding-top:16px; margin-bottom:20px;}


#header .logo{float:left; margin-top:23px; width:200px; position:relative; z-index:1000; transition:margin-top 0.3s ease-in-out }
#header .logo a{display:block; width:100%} 
#header .logo .off{display:block; width:100%}
#header .logo .on{display:none; width:100%}

#header .h_bot{height:70px;}

#header .h_utill{top:50%; right:0px; position:absolute; z-index:1000; 
-webkit-transform:translateY(calc(-50% + 0px));
-ms-transform:translateY(calc(-50% + 0px));
transform:translateY(calc(-50% + 0px));}



#header .gnb_menu{ background:transparent; padding:0px; width:22px;  height:22px; display:inline-block; vertical-align:middle; display:inline-block}



#header .gnb_menu.m_menu{display:none}
#header .gnb_menu.all_menu{display:block}
#header .all_menu .off{display:block}
#header .all_menu .on{display:none}
#header .all_menu.is-active .on{display:block}
#header .all_menu.is-active .off{display:none}

#header .m_menu{background:transparent}
#header .m_menu .off{display:block}
#header .m_menu .on{display:none}
#header .m_menu.is-active2 .on{display:block}
#header .m_menu.is-active2 .off{display:none}


#header .gnb_menu.all_menu .off{
filter:  brightness(100%);
-webkit-transition: filter 0.3s;
-o-transition: filter 0.3s;
transition: filter 0.3s;}

#header .gnb_menu.m_menu .off{
filter:  brightness(100%);
-webkit-transition: filter 0.3s;
-o-transition: filter 0.3s;
transition: filter 0.3s;}

/*::::::::::    gnb    ::::::::::::*/
#header .gnb_area{margin:0 auto;  width:920px;  display: block;}
#header .gnb{display:block; width:100%;}
#header .gnb > li{float:left; display:table; position:relative;  width:200px; margin-right:40px }




#header .gnb > li:last-child{margin-right:0px;}
#header .gnb > li > a{color: #fff; font-size:18px; display: table-cell;  z-index:406; position:relative;  height: 100px; padding:0px 25px; font-weight:500;
text-align: center; vertical-align: middle; word-break: keep-all;  
 transition:height 0.3s ease-in-out;}

#header .gnb > li > a span{z-index:407; position:relative; line-height:1; font-family: 'Poppins'; font-weight:500;}
#header .gnb > li > a span:before{content:""; display:block;  clear:both; height:5px; width:5px;
background:#003067; position:absolute; top:-1px; right:-9px;  z-index:406; border-radius:5px;
-webkit-transform:scale(0.0);
-ms-transform:scale(0.0);
transform:scale(0.0);
-webkit-transition: transform 0.3s;
-o-transition: transform .0.3s;
transition: transform 0.3s;} 

#header .gnb > li:hover > a:after{opacity:1;}

#header .gnb > li:hover > a span:before{
-webkit-transform:scale(1.0);
-ms-transform:scale(1.0);
transform:scale(1.0)}




/*depth*/
#header .depth_D{position:absolute; top:100px;  left:50%; transform:translateX(-50%); 
 content:""; width:100%;   padding:10px 0px; }
#header .depth_D li{position: relative; margin-bottom:10px; display:block;
letter-spacing:-0.05em; text-align:center; }
#header .depth_D li a{}
#header .depth_D li:last-child{margin-bottom:0px;}

#header .depth_D.gb{}
#header .depth_D.gb{display:block; transition: top 0.3s ease-in-out;
display: block;
visibility: hidden;
height: auto;
z-index: 400;}
#header .depth_D.gb li{z-index:401;}

#header .depth_D.on.gb:before{}
#header .depth_D .after{opacity:1}
#header.is-active .depth_D.on.gb:before{display:none;}


#header .depth_D.bg{display:none; 	background:#fff; transition:top 0.3s ease-in-out;
	-webkit-box-shadow: 0px 5px 5px rgb(0, 0, 0, 0.15);
    box-shadow: 0px 5px 5px rgb(0, 0, 0, 0.15);}
#header .depth_D.bg li{opacity:0;}

#header .depth_D li a{color:#333; padding:9px 0px;  opacity:0;  width:100%; display:block; font-size:18px;  position:relative;
-webkit-transform:translateY(15px);
-ms-transform:translateY(15px);
transform:translateY(15px);

 transition-property:  background, transform, opacity; 
transition-duration: 0.3s, 0.3s, 0.3s;
transition-timing-function: ease , ease, ease;
transition-delay: 0s, 0.3s, 0.3s}
#header .depth_D.gb.on{visibility: visible; opacity: 1;}
#header .depth_D.gb.on li a{opacity:1; 
-webkit-transform:translateY(0px);
-ms-transform:translateY(0px);
transform:translateY(0px);}
#header .depth_D.gb.on li:nth-child(1) a{transition-delay: 0s, 0.3s, 0.3s}
#header .depth_D.gb.on li:nth-child(2) a{transition-delay: 0s, 0.4s, 0.4s}
#header .depth_D.gb.on li:nth-child(3) a{transition-delay: 0s, 0.5s, 0.5s}
#header .depth_D.gb.on li:nth-child(4) a{transition-delay: 0s, 0.6s, 0.6s}
#header .depth_D.gb.on li:nth-child(5) a{transition-delay: 0s, 0.7s, 0.7s}
#header .depth_D.gb.on li:nth-child(6) a{transition-delay: 0s, 0.8s, 0.8s}
#header .depth_D.gb.on li:nth-child(7) a{transition-delay: 0s, 0.9s, 0.9s}
#header .depth_D.gb.on li:nth-child(8) a{transition-delay: 0s, 0.9s, 0.9s}


#header .depth_D li span{position:relative; }
#header .depth_D li:hover a{color:#fff; background:#003067; }
#header .depth_D li:hover::after{display:block;}
#header .depth_D li:hover span:after{display:block;}


/*:::::::: header on hover sub ::::::::*/
#header.on{background:#fff;
-webkit-box-shadow: 0px 2px 5px rgb(0, 0, 0, 0.15);
box-shadow: 0px 2px 5px rgb(0, 0, 0, 0.15);
height:70px;}


#header:hover{background:#fff; 
-webkit-box-shadow: 0px 2px 5px rgb(0, 0, 0, 0.15);
    box-shadow: 0px 2px 5px rgb(0, 0, 0, 0.15);}




/*:: h_top on sub  hover ::*/
#header.on .lang span{color:#040404;}
#header.on .lang span a{color:#040404;}
#header.on .lang .lang_depth li a{color:#040404;}
#header.on .lang .lang_depth li:hover a{color:#fff;}
#header.on .lang span:before{background-image:url('../images/ico/arr_off_on.png');}
#header.on .lang span.on:before{background-image:url('../images/ico/arr_open_on.png');}
#header.on .lang.family{border-right:1px solid #c3c3c3; border-left:1px solid #c3c3c3}
#header.on .lang.family span a{ color:#040404;}
#header.on .search{background-image:url('../images/ico/search_on.png')}
#header.on .depth_D{top:70px}
#header.on .depth_D{top:70px}
#header.on .depthBg{top:70px;}
#header.on .header_search{top:70px;}
#header.on .logo .off{display:none;}
#header.on .logo .on{display:block;}
#header.on .logo{margin-top:10px}
#header.on  .gnb > li > a{height:70px;}
#header.on .gnb_area .gnb > li > a{color:#040404;}
#header.on .gnb_menu.all_menu .off{filter: brightness(0%);;}
#header.on .gnb_menu.m_menu img{filter: brightness(0%);}

/*:: h_bot on hover sub  ::*/



#header:hover .lang span{color:#040404;}
#header:hover .lang span a{color:#040404;}
#header:hover .lang .lang_depth li a{color:#040404;}
#header:hover .lang .lang_depth li:hover a{color:#fff;}
#header:hover .lang span:before{background-image:url('../images/ico/arr_off_on.png');}
#header:hover .lang span.on:before{background-image:url('../images/ico/arr_open_on.png');}
#header:hover .lang.family{border-right:1px solid #c3c3c3; border-left:1px solid #c3c3c3}
#header:hover .lang.family span a{ color:#040404;}
#header:hover .search{background-image:url('../images/ico/search_on.png')}
#header:hover .logo .off{display:none;}
#header:hover .logo .on{display:block;}
#header:hover .gnb_area .gnb > li > a{color:#040404;}
#header:hover .gnb_menu.all_menu .off{filter: brightness(0%);}
#header:hover .gnb_menu.m_menu img{filter: brightness(0%);}
/*depthBg*/ 
#header .depthBg{position:absolute; left:0px; top:100px; width:100%; background:#fff;  z-index:402;
height:0px; overflow:hidden;  overflow:hidden;
 -webkit-transition: top 0.3s; 
    -o-transition: top 0.3s;
    transition: top 0.3s;
-webkit-box-shadow:0 3px 3px -3px rgba(0, 0, 0, 0.2); box-shadow:0 3px 3px -3px rgba(0, 0, 0, 0.2)}

#header.is-active .logo .off{display:block;}
#header.is-active .logo .on{display:none;}
#header.is-active{border-bottom:0px; box-shadow:none; background:transparent ;}
#header.is-active .gnb_area{opacity:0; 
-webkit-user-select:none;
  -moz-user-select:none;
  -ms-user-select:none;
  user-select:none}





